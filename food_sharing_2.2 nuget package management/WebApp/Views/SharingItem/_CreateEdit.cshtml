@model BLL.App.DTO.SharingItem

<div class="form-group">
    <label asp-for="SharingId" class="control-label"></label>
    <select asp-for="SharingId" class ="form-control" asp-items="ViewBag.SharingId"></select>
</div>
<div class="form-group">
    <label asp-for="ItemId" class="control-label"></label>
    <select asp-for="ItemId" class ="form-control" asp-items="ViewBag.ItemId"></select>
</div>
<div class="form-group">
    <label asp-for="FriendName" class="control-label"></label>
    <input asp-for="FriendName" class="form-control" />
    <span asp-validation-for="FriendName" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="Percent" class="control-label"></label>
    <input asp-for="Percent" class="form-control" />
    <span asp-validation-for="Percent" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="FriendOwns" class="control-label"></label>
    <input asp-for="FriendOwns" class="form-control" />
    <span asp-validation-for="FriendOwns" class="text-danger"></span>
</div>



