@model BLL.App.DTO.Pizza

<div class="form-group">
    <label asp-for="PizzaTemplateId" class="control-label"></label>
    <select asp-for="PizzaTemplateId" class ="form-control" asp-items="ViewBag.PizzaTemplateId"></select>
</div>
<div class="form-group">
    <label asp-for="SizeNumber" class="control-label"></label>
    <input asp-for="SizeNumber" class="form-control" />
    <span asp-validation-for="SizeNumber" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="SizeName" class="control-label"></label>
    <input asp-for="SizeName" class="form-control" />
    <span asp-validation-for="SizeName" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="Name" class="control-label"></label>
    <input asp-for="Name" class="form-control" />
    <span asp-validation-for="Name" class="text-danger"></span>
</div>


